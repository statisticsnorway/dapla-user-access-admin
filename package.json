{
  "name": "user-access-admin",
  "version": "0.2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/statisticsnorway/user-access-admin"
  },
  "dependencies": {
    "@statisticsnorway/ssb-component-library": "^2.0.7",
    "axios": "^0.19.2",
    "axios-hooks": "^1.9.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node ./node_modules/react-scripts/bin/react-scripts.js build",
    "test": "react-scripts test",
    "coverage": "node ./node_modules/react-scripts/bin/react-scripts.js test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.0",
    "@testing-library/user-event": "^10.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/index.js",
      "!src/**/index.js",
      "!src/configurations/**",
      "!src/enums/**",
      "!src/media/**",
      "!src/__tests__/test-data/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
